{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "paths": {
    "@prisma_/*": ["./src/prisma/*"],
    "@common/*": ["./src/common/*"],
    "@minio/*": ["./src/minio-storage/*"]
  }
}
